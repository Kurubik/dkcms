            <div data-pin-city="london" class="pin-map__city pin-map__city--london">
                <div  data-pin="click" class="pin__icon"></div>
                {% if pin[language ~ "_title"] %}
                    <div class="pin__text">{{ pin[language ~ "_title"] }}</div>
                {% endif %}
                <div class="pin__route pin__route--london">
                    {% include 'svg/routes/5.svg' %}
                </div>
                <div class="pin__modal">
