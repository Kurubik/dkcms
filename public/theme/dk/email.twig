<strong>Dubultu Krasts</strong>
<br><br>
<table border="0" cellpadding="0" cellspacing="0">
    {% if data.name != '' %}
        <tr style="height: 40px;border-collapse: collapse !important;">
            <td style="vertical-align: middle; width: 150px; padding: 10px 20px 10px 0;color: #242424;  border-bottom: 1px solid #3d4574;border-collapse: collapse !important;">
                Name:
            </td>
            <td style="vertical-align: middle; padding: 10px 0 10px 0; border-bottom: 1px solid #3d4574;border-collapse: collapse !important;">
                {{ data.name }}
            </td>
        </tr>
    {% endif %}
    {% if data.phone != '' %}
        <tr style="height: 40px;">
            <td style="vertical-align: middle; width: 150px; padding: 10px 20px 10px 0;color: #242424; border-bottom: 1px solid #3d4574;border-collapse: collapse !important;">
                Phone:
            </td>
            <td style="vertical-align: middle; padding: 10px 0 10px 0; border-bottom: 1px solid #3d4574;border-collapse: collapse !important;">
                {{ data.phone }}
            </td>
        </tr>
    {% endif %}
    {% if data.email != '' %}
        <tr style="height: 40px;">
            <td style="vertical-align: middle; width: 150px; padding: 10px 20px 10px 0;color: #242424; border-bottom: 1px solid #3d4574;border-collapse: collapse !important;">
                E-mail:
            </td>
            <td style="vertical-align: middle; padding: 10px 0 10px 0; border-bottom: 1px solid #3d4574;border-collapse: collapse !important;">
                {{ data.email }}
            </td>
        </tr>
    {% endif %}
</table>